{
  "buildCommand": "yarn build",
  "framework": "nextjs",
  "installCommand": "yarn install",
  "env": {
    "YARN_ENABLE_IMMUTABLE_INSTALLS": "false"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
